{
  "name": "justnote-html-for-pdf",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build-css": "npx tailwindcss -c tailwind.config.js -i tailwind.css -o css/tailwind.css",
    "watch-css": "npx tailwindcss -c tailwind.config.js -i tailwind.css -o css/tailwind.css --watch",
    "start": "live-server --ignore=node_modules,.git --no-browser"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "live-server": "git+https://github.com/tapio/live-server.git#ad22544",
    "tailwindcss": "^3.2.4"
  }
}
